        <div id="left" class="float-left">

            <div class="my_postition">

                <!-- serched-bld -->
                <div>

                    <form class="d-flex justify-content-end">
                        <input id="keyword" class="me-2" type="text" placeholder="위치를 정확하게 입력해주세요.">
                        <input id="wfs" type="hidden" value="${gangwonList.textList}">
                        <button id="btn-search" class="btn btn-primary" type="button">검색</button>
                    </form>

                    <div class="dropdown">
                        <button type="button" id="my_kw_genhosp"
